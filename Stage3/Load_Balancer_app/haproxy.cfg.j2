global
    log /dev/log local0
    log /dev/log local1 notice
    user haproxy
    group haproxy
    maxconn 4096
    stats socket /run/haproxy/admin.sock mode 660 level admin
    stats timeout 30s
    daemon

defaults
    log global
    mode tcp
    timeout connect 5000
    timeout client 50000
    timeout server 50000

frontend tcp-in
    bind 120.20.10.239:8080
    default_backend app-servers

backend app-servers
    balance roundrobin
    mode tcp
    server app-server01 120.20.10.238:8080 check
    server app-server03 120.20.10.233:8080 check
    server app-server02 120.20.10.234:8080 check
 
